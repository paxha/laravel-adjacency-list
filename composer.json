{
    "name": "paxha/laravel-adjacency-list",
    "description": "Recursive Laravel Eloquent relationships with CTEs",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Staudenmeir",
            "email": "mail@jonas-staudenmeir.de"
        },
        {
            "name": "Hassan Raza Pasha",
            "email": "e.hassanrazapasha@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/paxha/laravel-adjacency-list"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/database": "^6.0",
        "paxha/laravel-cte": "^2.0"
    },
    "require-dev": {
        "laravel/homestead": "^9.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Paxha\\LaravelAdjacencyList\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
